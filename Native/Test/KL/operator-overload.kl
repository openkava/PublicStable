struct Vec3
{
  Scalar x;
  Scalar y;
  Scalar z;
};

function Vec3 +( Vec3 lhs, Vec3 rhs )
{
  var Vec3 result;
  result.x = lhs.x + rhs.x;
  result.y = lhs.y + rhs.y;
  result.z = lhs.z + rhs.z;
  return result;
}

operator entry()
{
  var Vec3 vec3;
  vec3.x = 1;
