struct Vec3
{
  Scalar x;
  Scalar y;
  Scalar z;
};

function Vec3 doubleVec3( in Vec3 vec3 )
{
  var Vec3 result;
  result.x = 2 * vec3.x;
  result.y = 2 * vec3.y;
  result.z = 2 * vec3.z;
  return result;
}

function entry()
{
  var Vec3 vec3;
  vec3.x = 1;
  vec3.y = 2;
  vec3.z = 3;
  vec3 = doubleVec3( vec3 );
