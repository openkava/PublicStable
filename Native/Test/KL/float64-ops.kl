operator entry()
{
  var Float64 x;

  // Unary ops;
  x = 4.5;
  report +x;
  report -x;
  report !x;

  // Binary ops
  x = Float64(1.2) + Float64(5.6); report x;
  x = Float64(-5.2) - Float64(9.0); report x;
  x = Float64(9.4) * Float64(1.2); report x;
  x = Float64(-9.3) / Float64(5.4); report x;
  x = Float64(8.9) % Float64(2.1); report x;

  // Assignment ops
  x = Float64(7.1); report x;
  x += Float64(2.1); report x;
  x -= Float64(-4.7); report x;
  x *= Float64(4.5); report x;
  x /= Float64(-1.2); report x;
  x %= Float64(3.4); report x;
}
